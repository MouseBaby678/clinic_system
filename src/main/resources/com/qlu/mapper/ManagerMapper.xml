<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qlu.mapper.ManagerMapper">
    <select id="selectByUsernameAndPassword" resultType="com.qlu.bean.Manager">
        SELECT * FROM `manager` WHERE `manager_name`=#{manager_name} AND `manager_password`=#{manager_password}
    </select>
</mapper>